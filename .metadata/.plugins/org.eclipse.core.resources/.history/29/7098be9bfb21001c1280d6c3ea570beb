	<select id="selectMemeber" resultType="memberVO">
		SELECT user_no AS userNo
			,  user_id AS userId
			,  user_pw AS userPW 
			,  user_name AS userName
			,  auth_level AS authLevel
		  FROM dev_con.member
		 WHERE 0=0
	</select>
	
